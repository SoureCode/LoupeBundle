{
    "name": "sourecode/loupe-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "chapterjason",
            "email": "jason@sourecode.dev"
        }
    ],
    "require": {
        "loupe/loupe": "^0.10.3",
        "symfony/cache-contracts": "^3.5",
        "symfony/config": "^7.1",
        "symfony/dependency-injection": "^7.1",
        "symfony/filesystem": "^7.1",
        "symfony/finder": "^7.1",
        "symfony/http-kernel": "^7.1",
        "symfony/string": "^7.1",
        "symfony/translation-contracts": "^3.5"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.47",
        "nyholm/symfony-bundle-test": "^3.0",
        "php-cs-fixer/shim": "^3.75",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^7.1",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/runtime": "^7.1",
        "twig/twig": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "SoureCode\\Bundle\\Loupe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/app/src/",
            "SoureCode\\Bundle\\Loupe\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "symfony/runtime": true
        }
    }
}
